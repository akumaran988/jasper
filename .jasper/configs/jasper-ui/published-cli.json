{
  "llmProvider": "google-ai",
  "model": "gemini-2.5-flash-lite",
  "maxIterations": 12,
  "tokenLimit": 15000,
  "apiThrottleMs": 2000,
  "mcpServers": {
    "service-management": {
      "mode": "local",
      "autoStart": true,
      "httpUrl": "http://localhost:8081/mcp/tools",
      "description": "Built-in service management",
      "trust": true,
      "timeout": 30000,
      "serverConfig": {
        "port": 8081,
        "env": {
          "NODE_ENV": "development",
          "LOG_LEVEL": "info"
        }
      }
    },
    "file-management": {
      "mode": "local",
      "autoStart": false,
      "httpUrl": "http://localhost:8082/mcp/tools",
      "description": "Built-in file management",
      "trust": true,
      "timeout": 30000,
      "serverConfig": {
        "port": 8082,
        "env": {
          "NODE_ENV": "development"
        }
      }
    }
  },
  "serviceDefinitions": {
    "example-web-server": {
      "mcpServer": "service-management",
      "config": {
        "name": "web-server",
        "type": "process",
        "command": "python",
        "args": ["-m", "http.server", "8000"],
        "workingDir": ".",
        "healthCheck": {
          "url": "http://localhost:8000",
          "interval": 30
        },
        "autoRestart": false
      },
      "deployment": {
        "environment": "local"
      }
    },
    "example-docker-service": {
      "mcpServer": "service-management",
      "config": {
        "name": "nginx-server",
        "type": "docker",
        "image": "nginx:alpine",
        "ports": {
          "8080": "80"
        },
        "healthCheck": {
          "url": "http://localhost:8080",
          "interval": 30
        },
        "autoRestart": true
      },
      "deployment": {
        "environment": "local"
      }
    }
  },
  "deploymentProfiles": {
    "basic-examples": {
      "description": "Basic example services",
      "services": ["example-web-server"],
      "parallel": false,
      "autoStart": false
    },
    "docker-examples": {
      "description": "Docker container examples",
      "services": ["example-docker-service"],
      "parallel": false,
      "autoStart": false
    }
  }
}